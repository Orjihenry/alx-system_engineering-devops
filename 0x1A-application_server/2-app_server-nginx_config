server {
    listen 80;
    server_name ;

    location /airbnb-onepage/ {
        proxy_pass http://127.0.0.1:5000/airbnb-onepage/;
        proxy_set_header Host $host;
        add_header  X-Served-By 202392-web-01;
    }

    # error 404 page
    error_page 404 /404.html;
    location /404 {
        root /var/www/html;
        internal;
    }
}
